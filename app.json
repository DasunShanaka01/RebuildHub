{
  "expo": {
    "name": "RebuildHub",
    "slug": "RebuildHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rebuildhub",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBuKrghtMt7e6xdr3TLiGhVZNuqTFTgMXk"
        }
      },
      "package": "com.anonymous.RebuildHub",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to report damage incidents.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to report damage incidents."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-maps"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "entryPoint": "./node_modules/expo-router/entry"
  }
}
